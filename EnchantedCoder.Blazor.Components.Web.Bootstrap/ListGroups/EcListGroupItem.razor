@namespace EnchantedCoder.Blazor.Components.Web.Bootstrap

<div class="@GetClasses()"
	 aria-current="@Active"
	 aria-disabled="@(!Enabled)"
	 role="@(this.OnClick.HasDelegate ? "button" : null)"
	 @onclick="InvokeOnClickAsync"
	 @onclick:stopPropagation="this.OnClick.HasDelegate">
	@ChildContent
</div>